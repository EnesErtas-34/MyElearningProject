@model MyElearningProject.DAL.Entities.Address
@{
    ViewBag.Title = "AddAddress";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni Adres Girişi</h2>
<form method="post">
    <br />
    <label>Adres</label>
    <input type="text" name="AddressOfice" class="form-control" />
    <br /><br />
    <label>Telefon Numarası</label>
    <input type="number" name="Phone" class="form-control" />
    <br /><br />
    <label>Mail</label>
    <input type="email" name="Email" class="form-control" />
    <br /><br />
    <button class="btn btn-primary">Adresi Kaydet</button>
</form>

